import mongoose from 'mongoose';
const { Schema } = mongoose;

const [[name]]Schema = new Schema({
  name: { 
    type: String, 
    required: true 
  },
  // Add your schema fields here
}, {
  timestamps: true
});

const [[name]] = mongoose.models.[[name]] || mongoose.model('[[name]]', [[name]]Schema);

export default [[name]];